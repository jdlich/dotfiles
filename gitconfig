[push]
	default = simple
[user]
	name = Jacob Lichner
	email = jacob.d.lichner@gmail.com
[apply]
	whitespace = fix
[difftool "Kaleidoscope"]
	cmd = ksdiff-wrapper git \"$LOCAL\" \"$REMOTE\"
[difftool]
	prompt = false
[diff]
	tool = Kaleidoscope
[alias]
ws = "!git rebase --whitespace=fix --onto $(git merge-base HEAD @{u})"